{
  "expo": {
    "name": "Fruit Fusion",
    "slug": "FruitFusion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/FF.svg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/app-logo-extended.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ewelinaczr.FruitFusion",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/FF.svg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/app-logo-extended.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4ac40896-e927-4629-9507-c695cafef989"
      }
    },
    "owner": "ewelinaczr"
  }
}
